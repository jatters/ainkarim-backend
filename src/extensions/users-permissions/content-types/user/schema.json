{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "firstName": {
      "type": "string"
    },
    "middleName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "secondLastName": {
      "type": "string"
    },
    "document": {
      "type": "string"
    },
    "mobile": {
      "type": "biginteger"
    },
    "address": {
      "type": "string"
    },
    "department": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "reservas": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::reserva.reserva",
      "mappedBy": "users_permissions_user"
    },
    "pedidos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::pedido.pedido",
      "mappedBy": "users_permissions_user"
    },
    "allowMarketing": {
      "type": "boolean",
      "default": false
    },
    "documentType": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Cédula",
        "Cédula de extranjería",
        "Pasaporte",
        "NIT"
      ]
    },
    "bornDate": {
      "type": "date"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Masculino",
        "Femenino",
        "Otro"
      ]
    },
    "AgencyName": {
      "type": "string"
    },
    "AgencyTourismCode": {
      "type": "string"
    },
    "AgencyTourismCodeExpiration": {
      "type": "date"
    },
    "terms": {
      "type": "boolean"
    },
    "authorizedDiscount": {
      "type": "integer",
      "required": false,
      "default": 0,
      "max": 100
    },
    "AgencyRUT": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "AgencyCommerce": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "AgencyTourismRegister": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    }
  }
}
